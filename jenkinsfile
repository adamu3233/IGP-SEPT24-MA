pipeline
{
    agent any
    stages
    {
        stage('Code Checkout')
        {
            steps
            {
                git 'https://github.com/adamu3233/IGP-SEPT24-MA'
            }
        }
        
        stage ('Code Compile')
        {
            steps
            {
                sh 'mvn compile'
            }
        }
        
        stage ('Test')
        {
            steps
            {
                sh 'mvn test'
            }
        }
        
        stage ('Packaging')
        {
            steps
            {
                sh 'mvn package'
            }
        }
    }
}
